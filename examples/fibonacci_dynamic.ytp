extern fn int_printer(x: int) : void;

fn main() : int {
    let cache: int[50] = -1;
    let value : int = fib(40, cache);

    int_printer(value);
    return 0;
}


fn fib(n: int, cache: int[]) : int {
    if n <= 1 {
        return n;
    }

    if cache[n] != -1 {
         return cache[n];
    }

    cache[n] = fib(n-1, cache) + fib(n-2, cache);
    return cache[n];
      
}



